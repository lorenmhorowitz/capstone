{
  "name": "cornerstone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "parcel build ./src/index.html",
    "client": "parcel ./src/index.html -p 4000 --out-dir www --no-cache",
    "eject": "react-scripts eject",
    "lynt": "lynt src",
    "precommit": "npm run lynt && npm run pretty-quick && npm run test",
    "pretty-quick": "pretty-quick --staged",
    "start": "npm run client",
    "test": "jest --setupFiles ./testUtils/setup.js"
  },
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "material-ui-dots": "^2.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-responsive-carousel": "^3.1.43",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest": "^23.6.0",
    "lynt": "^0.5.3",
    "parcel-bundler": "^1.10.3",
    "pre-commit": "^1.2.2",
    "pretty-quick": "^1.8.0",
    "react-test-renderer": "^16.5.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "coveragePathIgnorePatterns": []
  },
  "precommit": [
    "lynt",
    "pretty-quick",
    "test"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
